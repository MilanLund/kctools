{
  "name": "kenticocloudtools",
  "version": "0.0.1",
  "description": "A boilerplate for Node.js web applications",
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Milan Lund",
  "license": "MIT",
  "scripts": {
    "start": "nodemon app.js",
    "test": "mocha --reporter spec",
    "eslint": "eslint **/*.js"
  },
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "chalk": "^2.1.0",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.0",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "dotenv": "^5.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.15.4",
    "express-flash": "^0.0.2",
    "express-session": "^1.15.5",
    "express-validator": "^5.0.0",
    "lusca": "^1.5.1",
    "mongoose": "^5.0.4",
    "morgan": "^1.8.2",
    "nodemailer": "^4.0.1",
    "nodemon": "^1.14.12",
    "passport": "0.4.0",
    "passport-local": "^1.0.0",
    "pug": "^2.0.0-rc.3",
    "request": "^2.83.0",
    "validator": "^9.4.0"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-chai-friendly": "^0.4.0",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^5.0.0",
    "sinon": "^2.1.0",
    "sinon-mongoose": "^2.0.2",
    "supertest": "^3.0.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "comma-dangle": 0,
      "consistent-return": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-shadow": 0,
      "no-console": 0,
      "no-plusplus": 0,
      "no-unused-expressions": 0,
      "chai-friendly/no-unused-expressions": 2
    },
    "env": {
      "jasmine": true,
      "mocha": true,
      "node": true
    },
    "plugins": [
      "chai-friendly"
    ]
  }
}
